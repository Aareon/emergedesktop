        <Library Value="emergeGraphics"/>
        <Library Value="emergeAppletEngine"/>
        <Library Value="emergeBaseClasses"/>
        <Library Value="stdc++_s"/>
      </Linker>
      <ResourceCompiler Options=""/>
    </GlobalSettings>
  </Settings>
