				<Option type="3" />
				<Option compiler="gcc" />
				<Option createDefFile="1" />
				<Option createStaticLib="1" />
				<Compiler>
					<Add option="-O2" />
					<Add option="-Wall" />
				</Compiler>
				<Linker>
					<Add option="-s" />
					<Add library="user32" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add option="-pedantic" />
			<Add option="-Wfatal-errors" />
			<Add option="-Wextra" />
			<Add option="-Wall" />
			<Add option="-Wno-unused-value" />
			<Add option="-Wno-strict-aliasing" />
			<Add option="-Wno-long-long" />
			<Add option="-DEMERGELIB_EXPORTS" />
